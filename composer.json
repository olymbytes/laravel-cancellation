{
    "name": "olymbytes/laravel-cancellation",
    "description": "A laravel cancellation package.",
    "keywords": [
        "olymbytes",
        "cancellation",
        "laravel"
    ],
    "homepage": "https://github.com/olymbytes/laravel-cancellation",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@foreno.dk"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/database": "^5.4",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "orchestra/database": "^3.4",
        "orchestra/testbench": "^3.4",
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Olymbytes\\Cancellation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olymbytes\\Cancellation\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config":  {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Olymbytes\\Cancellation\\CancellationServiceProvider"
            ]
        }
    }
}
